<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dyslexsee.dyslexsee">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.vision.DEPENCENCIES"
            android:value="ocr" />

        <activity android:name=".Revise" />
        <activity android:name=".Learn" />
        <activity android:name=".LearnQ2" />
        <activity android:name=".LearnQ3" />
        <activity android:name=".LearnQ4" />
        <activity android:name=".LearnQ5" />
        <activity android:name=".AfterActivity" />
        <activity android:name=".Record" />
        <activity android:name=".ParentMenu" />
        <activity android:name=".AddWord" />
        <activity android:name=".ReviseWrite" />
        <activity android:name=".BeforeRevise" />
        <activity android:name=".Learn2" />
        <activity android:name=".BeforeLearn" />
        <activity android:name=".beforeQ4"></activity>
    </application>

</manifest>